
@model List<Setting>
@{
    int count = 0;
}
<div class="container">
<table class="table">
  <thead>
    <tr>
      <th scope="col">No#</th>
      <th scope="col">Key</th>
      <th scope="col">Value</th>

    </tr>
  </thead>
  <tbody>
            @foreach (var item in Model)
            {
                count++;
                <tr>
                    <th scope="row">@count</th>
                    <td>@item.Key</td>
                    <td>@item.Value</td>
                    <td>
                        <a asp-controller="Setting" asp-action="Update" asp-route-id="@item.Id" class="btn btn-warning"> Update</a>
                        <a asp-controller="Setting" asp-action="Delete" asp-route-id="@item.Id" class="btn btn-danger"> Delete</a>

                    </td>
                </tr>
            }

  </tbody>
</table>
</div>